class Solution {
    // 66 Easy 加一 plus-one
    public int[] plusOne(int[] digits) {
        for (int i = digits.length - 1; i >= 0; i--) {
            if (digits[i] != 9) {
                digits[i]++;
                return digits;
            } else {
                digits[i] = 0;
            }
        }
        // 此时数组中全是9
        int[] digits2 = new int[digits.length + 1];
        digits2[0] = 1;
        return digits2;
    }
}
